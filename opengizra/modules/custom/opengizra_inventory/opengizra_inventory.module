include_once 'opengizra_inventory.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function opengizra_inventory_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools') {
    return 'plugins/' . $plugin;
  }
}
